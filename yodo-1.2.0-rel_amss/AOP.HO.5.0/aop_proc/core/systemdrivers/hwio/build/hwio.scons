import os

Import('env')
env = env.Clone()


if env['MSM_ID'] in ['waipio']:
	CHIPSET = env['CHIPSET_PMIC']
else:
	CHIPSET = env['CHIPSET']
	
SRCPATHSCRIPTS = env['BUILD_ROOT'] + '/core/systemdrivers/hwio/scripts/'

try:
  env.AddCMMScripts ('AOP', [SRCPATHSCRIPTS], {
    'HWIORegisters.cmm' : ["Registers - %s" % CHIPSET.upper(), CHIPSET]
  }, 'SystemDrivers')
except:
  pass

