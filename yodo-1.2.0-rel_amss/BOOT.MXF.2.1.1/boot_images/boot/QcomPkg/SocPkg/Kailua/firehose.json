{
    "Components": [{
            "Name": "XBL_SC",
            "Binary": "$BOOTROOT/$TARGETROOT/Bin_ext/$VAR/QC_sign/xbl_sc.elf"
        },
        {
            "Name": "TME_FW",
            "Binary": "$BOOTROOT/$TARGETROOT/Bin_ext/$VAR/QC_sign/signed_firmware_soc_view.elf"
        },
        {
            "Name": "TME_SEQ_RAM",
            "Binary": "$BOOTROOT/$TARGETROOT/Bin_ext/$VAR/sequencer_ram.elf"
        },
        {
            "Name": "DevPrg_ddr",
            "Process": [{
                    "ToolChain": "CLANG140",
                    "AArch": "AARCH64",
                    "Params": ["$EDK2ROOT/BaseTools/Source/Python/build/build.py", "-p", "QcomPkg/SocPkg/Kailua/Common/DevprogD.dsc", "--verbose", "-j", "QcomPkg/SocPkg/Kailua/$VAR/build_DevprogD.log", "-w", "-a", "AARCH64", "-b", "$REL"]
                }
            ]
        },
        {
            "Name": "DevPrgS_ddr",
            "Process": [{
                    "ToolChain": "CLANG140",
                    "AArch": "AARCH64",
                    "Params": ["$EDK2ROOT/BaseTools/Source/Python/build/build.py", "-p", "QcomPkg/SocPkg/Kailua/Common/DevprogDS.dsc", "--verbose", "-j", "QcomPkg/SocPkg/Kailua/$VAR/build_DevprogDS.log", "-w", "-a", "AARCH64", "-b", "$REL"]
                }
            ]
        },
        {
            "Name": "DevPrgST_ddr",
            "Process": [{
                    "ToolChain": "CLANG140",
                    "AArch": "AARCH64",
                    "Params": ["$EDK2ROOT/BaseTools/Source/Python/build/build.py", "-p", "QcomPkg/SocPkg/Kailua/Common/DevprogDST.dsc", "--verbose", "-j", "QcomPkg/SocPkg/Kailua/$VAR/build_DevprogDST.log", "-w", "-a", "AARCH64", "-b", "$REL"]
                }
            ]
        }, {
            "Name": "DTS",
            "Process": [{
                    "ToolChain": "Gendtb",
                    "Createfolders": ["$BUILDROOT/Build/Kailua$VAR/DTC"],
                    "Params": ["-f", "$BOOTROOT/Settings/Kailua/soc-kailua-1.0.dts", "-o", "$BUILDROOT/Build/Kailua$VAR/DTC/", "-c", "$BUILDROOT/../../tools/llvm/release/arm/14.0.0/bin", "-d", "$BUILDROOT/../../tools/dtc/1.6.1", "-i", "Settings/Kailua/Include Settings/Include"]
                },
                {
                    "ToolChain": "Gendtb",
                    "Createfolders": ["$BUILDROOT/Build/Kailua$VAR/DTC"],
                    "Params": ["-f", "$BOOTROOT/Settings/Kailua/pre-ddr-soc-kailua-1.0.dts", "-o", "$BUILDROOT/Build/Kailua$VAR/DTC/", "-c", "$BUILDROOT/../../tools/llvm/release/arm/14.0.0/bin", "-d", "$BUILDROOT/../../tools/dtc/1.6.1", "-i", "Settings/Kailua/Include Settings/Include"]
                },
                {
                    "ToolChain": "qc_dt_schema_check",
					"Createfolders": ["$BUILDROOT/Build/Kailua$VAR/DTSCHEMA/QC_DT_SCHEMA_CHECK"],
                    "Params": [
                               "-d", "$BUILDROOT/../../tools/dtc/1.6.1",
                               "-p", "$BUILDROOT/../../tools/llvm/release/arm/14.0.0/bin",
                               "-y", "$BOOTROOT/Settings/dtschema/Common",
                               "-t", "$BOOTTOOLS/dtschema_tools/bin",
							   "-o", "$BUILDROOT/Build/Kailua$VAR/DTSCHEMA/QC_DT_SCHEMA_CHECK",
							   "-r", "project_schema.yaml",
                               "-v",
                               "-n", "$BUILDROOT/../../tools/python/3.10.6/bin",
                               "-c"
                              ]
                },
                {
                    "ToolChain": "qc_dt_check",
					"Createfolders": ["$BUILDROOT/Build/Kailua$VAR/DTSCHEMA/QC_DT_CHECK"],
                    "Params": [
                               "-d", "$BUILDROOT/../../tools/dtc/1.6.1",
                               "-p", "$BUILDROOT/../../tools/llvm/release/arm/14.0.0/bin",
                               "-u", "$BUILDROOT/Build/Kailua$VAR/DTC/", 
                               "-r", "$BUILDROOT/Build/Kailua$VAR/DTSCHEMA/QC_DT_SCHEMA_CHECK/project_schema.yaml",
                               "-t", "$BOOTTOOLS/dtschema_tools/bin",
                               "-i", "$BOOTROOT/Settings/Kailua/Include", "$BOOTROOT/Settings/Include",
							   "-o", "$BUILDROOT/Build/Kailua$VAR/DTSCHEMA/QC_DT_CHECK",
                               "-v"
                              ]
                }
            ]
        }
    ],
    "CreateImages": [
        {
            "Name": "XBL_S_DEVPRG_NS",
            "Process": [{
                    "ToolChain": "GenXBLConfig",
                    "Components": ["DTS"],
                    "Createfolders": ["$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL", "$BUILDROOT/Build/Kailua$VAR/xblconfigdevprg"],
                    "Params": ["-i", "$BOOTROOT/$TARGETROOT/$VAR/create_xbl_config_dev_prg.json",
                        "-b", "$BUILDROOT",
                        "--sectools_directory", "$SECTOOLSROOT",
                        "--tools_path", "$BOOTTOOLS/XBLConfig",
                        "-f", "ELF",
                        "--output-file", "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/xbl_config_devprg",
                        "--autogen_directory", "$BUILDROOT/Build/Kailua$VAR/xblconfigdevprg",
                        "--elf-address", "0x1494E000",
                        "--security_profile", "$BUILDROOT/ssg_tmefwrel/profiles/kailua/kailua_tme_security_profile.xml",
                        "--sign_id", "XBL-CONFIG",
                        "--sectools_version", "v2",
                        "--signing_mode","TEST"]
                }, {
                    "ToolChain": "Copy",
                    "Components": ["DevPrg_ddr"],
                    "Createfolders": ["$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/unsigned"],
                    "Params": ["$BUILDROOT/Build/Kailua$VAR/DevprogD/$REL_$COMPILER/$AARCH/DevPrgD.elf", "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/unsigned/prog_firehose_ddr.elf"]
                }, {
                    "ToolChain": "Network_Sign_v2",
                    "Params": [
                        "secure-image", "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/unsigned/prog_firehose_ddr.elf",
                        "--outfile", "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/prog_firehose_ddr.elf",
                        "--image-id", "DEVICE-PROGRAMMER",
                        "--security-profile", "$BUILDROOT/ssg_tmefwrel/profiles/kailua/kailua_tme_security_profile.xml",
                        "--sign",
                        "--signing-mode", "TEST"]
                }, {
                    "ToolChain": "Create_Melf",
                    "Components": ["XBL_SC", "TME_FW", "TME_SEQ_RAM", "DevPrg_ddr"],
                    "Params": ["-o",
                        "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/prog_firehose_ddr.melf",
                        "-f",
                        "$BOOTROOT/$TARGETROOT/Bin_ext/$VAR/sequencer_ram.elf,$BOOTROOT/$TARGETROOT/Bin_ext/$VAR/QC_sign/signed_firmware_soc_view.elf,$BOOTROOT/$TARGETROOT/Bin_ext/$VAR/QC_sign/xbl_sc.elf,$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/prog_firehose_ddr.elf,$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/xbl_config_devprg.elf"
                        ]
                    }
                ]
            },
        {
            "Name": "XBL_S_DEVPRGS_NS",
            "Process": [{
                    "ToolChain": "GenXBLConfig",
                    "Components": ["DTS"],
                    "Createfolders": ["$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL", "$BUILDROOT/Build/Kailua$VAR/xblconfigdevprg"],
                    "Params": ["-i", "$BOOTROOT/$TARGETROOT/$VAR/create_xbl_config_dev_prg.json",
                        "-b", "$BUILDROOT",
                        "--sectools_directory", "$SECTOOLSROOT",
                        "--tools_path", "$BOOTTOOLS/XBLConfig",
                        "-f", "ELF",
                        "--output-file", "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/xbl_config_devprg",
                        "--autogen_directory", "$BUILDROOT/Build/Kailua$VAR/xblconfigdevprg",
                        "--elf-address", "0x1494E000",
                        "--security_profile", "$BUILDROOT/ssg_tmefwrel/profiles/kailua/kailua_tme_security_profile.xml",
                        "--sign_id", "XBL-CONFIG",
                        "--sectools_version", "v2",
                        "--signing_mode","TEST"]
                }, {
                    "ToolChain": "Copy",
                    "Components": ["DevPrgS_ddr"],
                    "Createfolders": ["$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/unsigned"],
                    "Params": ["$BUILDROOT/Build/Kailua$VAR/DevprogDS/$REL_$COMPILER/$AARCH/DevPrgDS.elf", "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/unsigned/prog_firehose_somc_ddr_live.elf"]
                }, {
                    "ToolChain": "Network_Sign_v2",
                    "Params": [
                        "secure-image", "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/unsigned/prog_firehose_somc_ddr_live.elf",
                        "--outfile", "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/prog_firehose_somc_ddr_live.elf",
                        "--image-id", "DEVICE-PROGRAMMER",
                        "--security-profile", "$BUILDROOT/ssg_tmefwrel/profiles/kailua/kailua_tme_security_profile.xml",
                        "--sign",
                        "--signing-mode", "TEST"]
                }, {
                    "ToolChain": "Create_Melf",
                    "Components": ["XBL_SC", "TME_FW", "TME_SEQ_RAM", "DevPrgS_ddr"],
                    "Params": ["-o",
                        "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/prog_firehose_somc_ddr_live.melf",
                        "-f",
                        "$BOOTROOT/$TARGETROOT/Bin_ext/$VAR/sequencer_ram.elf,$BOOTROOT/$TARGETROOT/Bin_ext/$VAR/QC_sign/signed_firmware_soc_view.elf,$BOOTROOT/$TARGETROOT/Bin_ext/$VAR/QC_sign/xbl_sc.elf,$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/prog_firehose_somc_ddr_live.elf,$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/xbl_config_devprg.elf"
                        ]
                    }
                ]
            },
        {
            "Name": "XBL_S_DEVPRGST_NS",
            "Process": [{
                    "ToolChain": "GenXBLConfig",
                    "Components": ["DTS"],
                    "Createfolders": ["$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL", "$BUILDROOT/Build/Kailua$VAR/xblconfigdevprg"],
                    "Params": ["-i", "$BOOTROOT/$TARGETROOT/$VAR/create_xbl_config_dev_prg.json",
                        "-b", "$BUILDROOT",
                        "--sectools_directory", "$SECTOOLSROOT",
                        "--tools_path", "$BOOTTOOLS/XBLConfig",
                        "-f", "ELF",
                        "--output-file", "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/xbl_config_devprg",
                        "--autogen_directory", "$BUILDROOT/Build/Kailua$VAR/xblconfigdevprg",
                        "--elf-address", "0x1494E000",
                        "--security_profile", "$BUILDROOT/ssg_tmefwrel/profiles/kailua/kailua_tme_security_profile.xml",
                        "--sign_id", "XBL-CONFIG",
                        "--sectools_version", "v2",
                        "--signing_mode","TEST"]
                }, {
                    "ToolChain": "Copy",
                    "Components": ["DevPrgST_ddr"],
                    "Createfolders": ["$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/unsigned"],
                    "Params": ["$BUILDROOT/Build/Kailua$VAR/DevprogDST/$REL_$COMPILER/$AARCH/DevPrgDST.elf", "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/unsigned/prog_firehose_somc_ddr_test.elf"]
                }, {
                    "ToolChain": "Network_Sign_v2",
                    "Params": [
                        "secure-image", "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/unsigned/prog_firehose_somc_ddr_test.elf",
                        "--outfile", "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/prog_firehose_somc_ddr_test.elf",
                        "--image-id", "DEVICE-PROGRAMMER",
                        "--security-profile", "$BUILDROOT/ssg_tmefwrel/profiles/kailua/kailua_tme_security_profile.xml",
                        "--sign",
                        "--signing-mode", "TEST"]
                }, {
                    "ToolChain": "Create_Melf",
                    "Components": ["XBL_SC", "TME_FW", "TME_SEQ_RAM", "DevPrgST_ddr"],
                    "Params": ["-o",
                        "$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/prog_firehose_somc_ddr_test.melf",
                        "-f",
                        "$BOOTROOT/$TARGETROOT/Bin_ext/$VAR/sequencer_ram.elf,$BOOTROOT/$TARGETROOT/Bin_ext/$VAR/QC_sign/signed_firmware_soc_view.elf,$BOOTROOT/$TARGETROOT/Bin_ext/$VAR/QC_sign/xbl_sc.elf,$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/prog_firehose_somc_ddr_test.elf,$BOOTROOT/$TARGETROOT/Bin/$VAR/$REL/xbl_config_devprg.elf"
                        ]
                    }
                ]
            }

    ]
}
