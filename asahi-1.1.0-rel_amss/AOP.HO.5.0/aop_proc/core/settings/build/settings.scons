Import('env')
env = env.Clone()

if env['MSM_ID'] not in ['waipio','fillmore','kailua','aurora','lassen','netrani', 'lanai', 'pakala','kapiti','kuno','pinnacle', 'hamoa', 'camano', 'palawan', 'matrix', 'milos', 'kaanapali']: #Enable list
  Return()

# Sources
SETTINGS_SRCS = [
    '${BUILD_ROOT}/core/settings/src/aop_settings.c',
]

# APIs
CBSP_API = [
    'DAL',
    'POWER',
    'POWER_UTILS',
    'SERVICES',
    'DEBUGTRACE',
    'SYSTEMDRIVERS',
    'PMIC',
    'KERNEL',
    'UNPA',
    'MPROC',
    'DDR_MGR',
    'PWR_UTILS',
    'AOP_SETTINGS',
    'PMIC_SETTINGS'
]

env.RequirePublicApi(CBSP_API)
env.RequireRestrictedApi(CBSP_API)
env.RequireProtectedApi(CBSP_API)

# Add sources to compile
if env['MSM_ID'] in ['waipio']:
    env.AddBinaryLibrary(['CORE_AOP'], '${BUILDPATH}/${CHIPSET_PMIC}/settings', SETTINGS_SRCS)
else:
    env.AddBinaryLibrary(['CORE_AOP'], '${BUILDPATH}/${MSM_ID}/settings', SETTINGS_SRCS)
env.LoadSoftwareUnits() 
