module: { name = RPMH_PDC_AUDIO_AUDIO_PDC, group = PDC_EN }
regs:
    CMD_ENABLE_BANK = [0x7, 0x7, 0x7]
    TIMER_MATCH_VALUE_LO = 0xFFFFFFFF
    TIMER_MATCH_VALUE_HI = 0xFFFFFFFF
    ENABLE_PDC =
    {.ENABLE_PDC = 0x1}
