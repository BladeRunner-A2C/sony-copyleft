Import('env')

env = env.Clone()

if not env.PathExists("${INC_ROOT}/core/api/systemdrivers/clock/${CHIPSET}"):
  env['CHIPSET'] = 'stub'

#-------------------------------------------------------------------------------
# Publish Public APIs
# These are accessible internal or external to CoreBSP
#-------------------------------------------------------------------------------
if env['CHIPSET'] in ['waipio','fillmore']:
  env.PublishPublicApi('SYSTEMDEBUG', [
     "${INC_ROOT}/core/api/systemdebug",
     "${INC_ROOT}/core/api/kernel/libstd",
     "${INC_ROOT}/core/systemdebug/sysdbg/inc",
     "${INC_ROOT}/core/systemdebug/sysdbg/inc/target/${CHIPSET}"
  ])
else:
  env.PublishPublicApi('SYSTEMDEBUG', [
     "${INC_ROOT}/core/api/systemdebug",
     "${INC_ROOT}/core/api/kernel/libstd",
     "${INC_ROOT}/core/systemdebug/sysdbg/inc",
     "${INC_ROOT}/core/systemdebug/sysdbg/inc/target/${CHIPSET}"
  ])

