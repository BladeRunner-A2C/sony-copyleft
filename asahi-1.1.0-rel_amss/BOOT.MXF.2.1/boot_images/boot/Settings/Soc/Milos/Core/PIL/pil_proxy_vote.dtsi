#include "../../Include/pil_proxy_vote_defines.h"

&pil_proxy_vote {


  compatible = "qcom,pil-proxy-vote";

  pil_proxy_vote_1 {
    subsys_id                = <TZ_PIL_AUTH_QDSP6_PROC>;
