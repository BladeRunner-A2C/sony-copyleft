#include "smem_host_type.h"
#include "smp2p_defines.h"

&sw{
  smp2p {
        compatible = "qcom,smp2p";
		fflags = <SMP2P_FFLAGS_SSR_HANDSHAKE>;
        max-entries = <0x10>;
        smp2p-interrupts {
		    intr-01{
		    dest = <SMEM_ADSP>; 
            proc = <IPCC_C_LPASS>;
            irq = <SMP2P_IRQ>;
			};
			intr-02{
		    dest = <SMEM_CDSP>; 
            proc = <IPCC_C_CDSP>;
            irq = <SMP2P_IRQ>;
			};
			intr-03{
		    dest = <SMEM_MODEM>; 
            proc = <IPCC_C_MPSS>;
            irq = <SMP2P_IRQ>;
			};
			intr-04{
		    dest = <SMEM_WPSS>; 
            proc = <IPCC_C_WPSS>;
            irq = <SMP2P_IRQ>;
			};
			intr-05{
		    dest = <SMEM_INVALID_HOST>; 
            proc = <SMP2P_NULL_CLIENT>;
            irq = <SMP2P_NULL_SIGNAL>;
			};
        };
	};
};