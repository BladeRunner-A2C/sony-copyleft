[Defines]
  INF_VERSION                    = 0x00010005
  BASE_NAME                      = ExtDriversNonSecureLib
  FILE_GUID                      = 2878BE06-3CB7-41AD-AC22-57942C97C9AB
  MODULE_TYPE                    = BASE
  VERSION_STRING                 = 1.0
  LIBRARY_CLASS                  = ExtDriversNonSecureLib

[Sources.common]
  boot_bam.c
  boot_busywait.c
  boot_clk.c
  boot_coldplug.c
  boot_crypto.c
  boot_chipinfo.c
  boot_dal.c
  boot_ddr.c
  boot_ddr_info.c
  boot_edk2_armlib.c
  boot_edk2_cache.c
  boot_edk2_mmu.c
  boot_eeprom.c
  boot_efs.c
  boot_mhi.c
  boot_pcie.c
  boot_platforminfo.c
  boot_pmic.c
  boot_power.c
  boot_qsee.c
  boot_qusb_al_bulk.c
  boot_qusb_ldr.c
  boot_ramdump.c
  boot_seclib_ns.c
  boot_smem.c
  boot_timer_hw.c
  boot_uart.c
  boot_visual_indication.c
  boot_dbginfo.c
  boot_nvme.c

[BuildOptions.AARCH64]
  GCC:*_*_*_CC_FLAGS = -Werror -DFEATURE_SAHARA_BOOT -DSAHARA_MAX_PACKET_SIZE_IN_BYTES=0x400 -DSAHARA_MAX_MEMORY_DATA_SIZE_IN_BYTES=0x1000 -DSAHARA_PACKET_LOG_LENGTH=0x64 -DUSE_GNU_LD -DUSE_LOADER_UTILS -DFEATURE_BOOT_LOGGER_RAM -DFEATURE_BOOT_LOGGER_TIMER -DFEATURE_BOOT_LOGGER_JTAG -DFEATURE_BOOT_LOGGER_UART -DBOOT_MEMORY_DEBUG_64BIT

[BuildOptions.ARM]
  GCC:*_*_*_CC_FLAGS = -Werror

[Packages]
  ArmPkg/ArmPkg.dec
  MdePkg/MdePkg.dec
  QcomPkg/XBLLoaderApi.dec
  QcomPkg/XBLLoader/ExtDrivers/ExtDrivers.dec
  QcomPkg/XBLLoader/SmcLib/SmcLib.dec
  QcomSdkPkg/SdkLoader.dec
  SecPkg/SecLib.dec
  QcomPkg/SocPkg/Library/BootPblSharedDataLib/BootPblSharedDataLib.dec
  SecPkg/UCLib.dec
  Settings/DeviceTreeSettings.dec
  QcomPkg/LoaderFramework/LoaderFrameworkApi.dec
  QcomPkg/Library/MinidumpLib/MinidumpLib.dec
  SecPkg/DbgInfoLib.dec

[LibraryClasses.common]
  TimerLib
  BootPblSharedDataLib
  ColdplugLib
  DbgInfoLib
  DDRTargetLib
  EepromCommonLib
  HashLib
  NvmeLoaderLib
