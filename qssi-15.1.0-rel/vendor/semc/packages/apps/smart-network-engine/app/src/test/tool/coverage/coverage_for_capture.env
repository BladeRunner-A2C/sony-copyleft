#!/bin/bash

if [ $# -ne 1 ]; then
    return 1
fi

if [ -z $SCRIPT_DIR ]; then
    return 1
fi

source coverage.env

NAME_SUFFIX=$1
export LCOV_CAPTURE_FILE="${OUTPUT_DIR}/lcov_${NAME_SUFFIX}.info"
export LCOV_TMP_FILTERED_FILE="${OUTPUT_DIR}/${FILTERED_PREFIX}_tmp_${NAME_SUFFIX}.info"
export LCOV_FILTERED_FILE="${OUTPUT_DIR}/${FILTERED_PREFIX}_${NAME_SUFFIX}.info"

