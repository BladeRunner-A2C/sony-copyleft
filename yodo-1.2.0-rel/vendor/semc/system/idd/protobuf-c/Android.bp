}

license {
    name: "vendor_semc_system_idd_protobuf-c_license",
    visibility: [":__subpackages__"],
    license_kinds: [
        "SPDX-license-identifier-Apache-2.0",
    ],
    license_text: [
        "NOTICE",
    ],
}

//For vendor partition
cc_library_shared {
    name: "libprotobuf-c-idd",
    owner: "somc",
    proprietary: true,

    cflags: ["-Wall",
             "-O2",
             "-O3"],
    include_dirs: ["."],
    srcs: ["google/protobuf-c/protobuf-c.c"],
    export_include_dirs: ["."],

  }

//For system partition
cc_library_shared {
    name: "libprotobuf-c-plat",
    product_specific: true,
    cflags: ["-Wall",
             "-O2",
             "-O3"],
    include_dirs: ["."],
    srcs: ["google/protobuf-c/protobuf-c.c"],
    export_include_dirs: ["."],

  }

cc_library_static {
    name: "libprotobuf-c_static",
    cflags: ["-Wall",
             "-O2",
             "-O3"],
    srcs: ["google/protobuf-c/protobuf-c.c"],
    include_dirs: ["."],
    export_include_dirs: ["."],

  }
